  },
  "homepage": "https://github.com/browserstack/webdriverio-browserstack#readme",
  "dependencies": {
    "@percy/cli": "^1.28.7",
    "@percy/selenium-webdriver": "^2.1.0",
    "@percy/webdriverio": "^3.1.1",
    "@wdio/cli": "7.27.0",
    "@wdio/cucumber-framework": "7.27.0",
    "chai": "^4.3.6",
    "webdriverio": "7.27.0"
  },
  "devDependencies": {
    "@wdio/browserstack-service": "7.27.0",
    "@wdio/local-runner": "7.27.0",
    "@wdio/mocha-framework": "7.20.3",
    "wdio-safaridriver-service": "^2.1.1"
