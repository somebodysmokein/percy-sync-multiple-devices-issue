  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserstack/webdriverio-browserstack.git"
  },
  "keywords": [
    "webdriverio",
    "browserstack",
    "selenium",
    "tests"
  ],
  "bugs": {
